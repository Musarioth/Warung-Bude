#include <stdio.h>
#include <stdlib.h>

struct Dish {
    char *name;
    int price, quantity;
    Dish *prev, *next;
}*head_dish, *tail_dish, *curr_dish, *new_dish, *del_dish;

struct Node_Dish{
    Dish dish;
    char *name;
    int price, quantity;
    Node_Dish *prev, *next, *index;
}*dish_head, *dish_tail, *dish_curr;
